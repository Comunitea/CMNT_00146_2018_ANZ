<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Add custom styles -->
    <template id="website_assets_frontend" inherit_id="website_multi_theme.auto_assets_website_umb">
        <xpath expr="link[last()]" position="after">
            <link href="/theme_umbro/static/css/_general.less" rel="stylesheet" type="text/less"/>
            <t t-call="theme_anzamar.custom_css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_anzamar/static/js/main.js"/>
        </xpath>
    </template>

</odoo>