<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="theme_feel_point_website_layout" inherit_id="website_multi_theme.auto_layout_website_fee">
        <xpath expr="//div[@id='wrapwrap']//div[hasclass('navbar')]" position="before">
            <t t-call="theme_anzamar.custom_header"/>
        </xpath>
    </template>

    <template id="theme_feel_logo_show" inherit_id="website_multi_theme.auto_view_fee_website_layout_logo_show">
        <xpath expr="//a[hasclass('navbar-brand')]/span" position="attributes">
            <attribute name="t-field">website.favicon</attribute>
        </xpath>
    </template>

</odoo>