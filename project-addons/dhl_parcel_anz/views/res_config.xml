<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_dhl_form" model="ir.ui.view">
        <field name="name">res.config.settings.dhl.form</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="model">res.config.settings</field>
        <field name="arch" type="xml">
            <xpath expr="//h2[@id='schedule_info']" position="before">
                <h2>DHL Config</h2>
                <div class="o_settings_container">
                    <group>
                        <field name="dhl_client_number_b2b"/>
                        <field name="dhl_client_number_b2c"/>
                    </group>
                </div>
            </xpath>
        </field>
    </record>

</odoo>